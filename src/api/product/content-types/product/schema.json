{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "desc": {
      "type": "richtext"
    },
    "offer": {
      "type": "richtext"
    },
    "price": {
      "type": "string",
      "required": true
    },
    "rating": {
      "type": "decimal"
    },
    "owner": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "room",
        "bike",
        "food",
        "water"
      ],
      "default": "room"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "intime": {
      "type": "string"
    },
    "outtime": {
      "type": "string"
    },
    "extraRules": {
      "type": "string"
    }
  }
}
